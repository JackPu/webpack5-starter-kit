    "@babel/register": "7.14.5",
    "@babel/runtime": "7.14.6",
    "autoprefixer": "10.2.6",
    "ava": "3.15.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "chalk": "4.1.1",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "cssnano": "5.0.6",
    "cssnano-preset-advanced": "5.1.3",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
