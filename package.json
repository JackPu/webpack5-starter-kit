{
  "name": "webpack5-starter-kit",
  "version": "2.0.0",
  "description": "Webpack 5 starter kit",
  "main": "./src/index.js",
  "scripts": {
    "dev": "node ./config/dev",
    "build": "node ./config/prod",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "nyc ava",
    "prestart": "yarn build",
    "start": "http-server ./dist -p 8082 --silent"
  },
  "author": "Giorgio Gamberoni <giorgio.gamberoni@gmail.com> (https://github.com/king-prawns)",
  "license": "MIT",
  "homepage": "https://github.com/king-prawns/webpack5-starter-kit#readme",
  "ava": {
    "files": [
      "./src/**/*test.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-flow": "7.16.0",
    "@babel/register": "7.16.0",
    "@babel/runtime": "7.16.3",
    "autoprefixer": "10.4.0",
    "ava": "3.15.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "chalk": "4.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.0.0",
    "css-loader": "6.5.1",
    "cssnano": "5.0.11",
    "cssnano-preset-advanced": "5.1.7",
    "eslint": "8.3.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "file-loader": "6.2.0",
    "flow-bin": "0.165.1",
    "fs-extra": "10.0.0",
    "html-loader": "3.0.1",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.0.0",
    "mini-css-extract-plugin": "2.4.5",
    "nyc": "15.1.0",
    "opn": "6.0.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss": "8.3.11",
    "postcss-easy-import": "3.0.0",
    "postcss-load-config": "3.1.0",
    "postcss-loader": "6.2.0",
    "postcss-preset-env": "7.0.1",
    "postcss-reporter": "7.0.4",
    "postcss-responsive-type": "1.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.1.0",
    "stylelint-config-standard": "24.0.0",
    "stylelint-webpack-plugin": "3.1.0",
    "terser-webpack-plugin": "5.2.5",
    "url-loader": "4.1.1",
    "webpack": "5.64.2",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.5.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.16.3"
  }
}
