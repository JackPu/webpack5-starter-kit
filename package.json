    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.0.3",
    "css-loader": "4.2.1",
    "cssnano": "4.1.10",
    "cssnano-preset-advanced": "4.0.7",
    "eslint": "7.6.0",
